<div class="container">

  <div>
    <h1> All Reviews </h1>
  </div>

  <% @reviews.each do |review| %>
    <div class="card">
      <h3> <%= review.content %> </h3>
      <p> <%= review.rating %> </p>
      <p> <%= review.restaurant.name %></p>
    </div>
  <% end %>

  <div>
    <p> testing </p>
  </div>

</div>
